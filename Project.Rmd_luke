df8 <- read.csv('C:/Users/Luke-Workstation/Desktop/ADS 503/ADS-503-Team-3/cleaned/foundational_reading.csv')
df9 <- read.csv('C:/Users/Luke-Workstation/Desktop/ADS 503/ADS-503-Team-3/cleaned/foundational_numeracy.csv')
head(df1)
